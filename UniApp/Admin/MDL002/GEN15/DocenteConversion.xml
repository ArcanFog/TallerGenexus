<Statements>
<Statement>ALTER TABLE [Docente] ADD [AsignaturaNombre]  char(20) NOT NULL CONSTRAINT AsignaturaNombreDocente_DEFAULT DEFAULT ''</Statement>
<Statement>UPDATE [Docente] SET [AsignaturaNombre]=T.[AsignaturaNombre] FROM (SELECT T1.[DocenteId], T2.[AsignaturaNombre] FROM ([Docente] T1 INNER JOIN [Asignatura] T2 ON T2.[AsignaturaId] = T1.[AsignaturaId])) T WHERE [Docente].DocenteId= T.DocenteId</Statement>
<Statement>ALTER TABLE [Docente] DROP CONSTRAINT AsignaturaNombreDocente_DEFAULT</Statement>
<Statement>DROP INDEX [IDOCENTE1] ON [Docente]</Statement>
<Statement>CREATE NONCLUSTERED INDEX [IDOCENTE1] ON [Docente] ([AsignaturaId] ,[AsignaturaNombre] )</Statement>
<Statement>ALTER TABLE [Docente] ADD CONSTRAINT [IDOCENTE1] FOREIGN KEY ([AsignaturaId], [AsignaturaNombre]) REFERENCES [Asignatura] ([AsignaturaId], [AsignaturaNombre])</Statement></Statements>
